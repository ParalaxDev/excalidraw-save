{
  "manifest_version": 3,
  "name": "Excalidraw Save",
  "version": "1.0.0",
  "description": "A simple extension to manage and save excalidraw saves",
  "permissions": [
    "tabs",
    "storage", "declarativeContent", "activeTab", "scripting"
  ],
  "host_permissions": [
    "http://excalidraw.com/*",
    "https://excalidraw.com/*"
  ],
  "action": {
    "default_popup": "index.html"
  }
}
